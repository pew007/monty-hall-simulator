<div>
    <table class="table table-bordered table-hover">
        <thead>
        <tr>
            <th class="table-primary"></th>
            <th class="table-primary">Change</th>
            <th class="table-primary">No Change</th>
        </tr>
        </thead>

        <tbody>
        <tr>
            <th class="table-primary">Wins</th>
            <td class="table-warning">{{ wins }}</td>
            <td class="table-warning">{{ losses }}</td>
        </tr>

        <tr>
            <th class="table-primary">Percentage</th>
            <td class="table-warning">{{ winPercentage | percentage }}</td>
            <td class="table-warning">{{ lossesPercentage | percentage }}</td>
        </tr>
        </tbody>
    </table>
</div>
